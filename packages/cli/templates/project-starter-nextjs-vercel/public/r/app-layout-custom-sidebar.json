  "title": "Custom Sidebar",
  "description": "Layout with a custom sidebar instead of the default AppSidebar",
  "files": [
    {
      "path": "./src/ui-registry/registry/examples/app-layout-custom-sidebar.tsx",
      "content": "\"use client\";\n\nimport { FileText, Home, Settings } from \"lucide-react\";\nimport {\n\tSidebar,\n\tSidebarContent,\n\tSidebarGroup,\n\tSidebarGroupLabel,\n\tSidebarHeader,\n\tSidebarMenu,\n\tSidebarMenuButton,\n\tSidebarMenuItem,\n} from \"@/components/ui/sidebar\";\nimport {\n\tAppLayout,\n\tAppLayoutContent,\n\tAppLayoutHeader,\n\tAppLayoutHeaderTitle,\n\tAppLayoutPage,\n} from \"@/ui-registry/registry/ui/app-layout\";\n\nexport function AppLayoutCustomSidebar() {\n\tconst customSidebar = (\n\t\t<Sidebar variant=\"inset\" collapsible=\"icon\">\n\t\t\t<SidebarHeader>\n\t\t\t\t<div className=\"flex items-center gap-2 px-4 py-2\">\n\t\t\t\t\t<div className=\"h-6 w-6 rounded bg-primary\" />\n\t\t\t\t\t<span className=\"font-semibold\">Custom App</span>\n\t\t\t\t</div>\n\t\t\t</SidebarHeader>\n\t\t\t<SidebarContent>\n\t\t\t\t<SidebarGroup>\n\t\t\t\t\t<SidebarGroupLabel>Navigation</SidebarGroupLabel>\n\t\t\t\t\t<SidebarMenu>\n\t\t\t\t\t\t<SidebarMenuItem>\n\t\t\t\t\t\t\t<SidebarMenuButton>\n\t\t\t\t\t\t\t\t<Home className=\"h-4 w-4\" />\n\t\t\t\t\t\t\t\t<span>Home</span>\n\t\t\t\t\t\t\t</SidebarMenuButton>\n\t\t\t\t\t\t</SidebarMenuItem>\n\t\t\t\t\t\t<SidebarMenuItem>\n\t\t\t\t\t\t\t<SidebarMenuButton>\n\t\t\t\t\t\t\t\t<FileText className=\"h-4 w-4\" />\n\t\t\t\t\t\t\t\t<span>Documents</span>\n\t\t\t\t\t\t\t</SidebarMenuButton>\n\t\t\t\t\t\t</SidebarMenuItem>\n\t\t\t\t\t\t<SidebarMenuItem>\n\t\t\t\t\t\t\t<SidebarMenuButton>\n\t\t\t\t\t\t\t\t<Settings className=\"h-4 w-4\" />\n\t\t\t\t\t\t\t\t<span>Settings</span>\n\t\t\t\t\t\t\t</SidebarMenuButton>\n\t\t\t\t\t\t</SidebarMenuItem>\n\t\t\t\t\t</SidebarMenu>\n\t\t\t\t</SidebarGroup>\n\t\t\t</SidebarContent>\n\t\t</Sidebar>\n\t);\n\n\treturn (\n\t\t<AppLayout sidebar={customSidebar}>\n\t\t\t<AppLayoutPage>\n\t\t\t\t<AppLayoutHeader>\n\t\t\t\t\t<AppLayoutHeaderTitle>Custom Sidebar</AppLayoutHeaderTitle>\n\t\t\t\t</AppLayoutHeader>\n\t\t\t\t<AppLayoutContent>\n\t\t\t\t\t<div className=\"p-6\">\n\t\t\t\t\t\t<h3 className=\"text-lg font-medium\">Content Area</h3>\n\t\t\t\t\t\t<p>This layout uses a custom sidebar instead of the default AppSidebar.</p>\n\t\t\t\t\t</div>\n\t\t\t\t</AppLayoutContent>\n\t\t\t</AppLayoutPage>\n\t\t</AppLayout>\n\t);\n}\n",
      "type": "registry:example"
    }
  ]
}